<div class="container">
  <div class="top-wrapper">
    <div id="video-area-index">
      <%= video_tag('/videos/surfmovie.mp4', autoplay: true, loop: true ) %>
    </div>
    <div class="typing">
     <p id="timer"></p>
     <p id="typeMiss"></p>
     <p id="targetJp">Let's Surf Typing!</p>
     <p id="targetEn"></p>
     <p id="result"></p>
      <%= link_to '', root_path, class: 'retry' %>
      <%= link_to "", target: :_blank, rel: 'noopener noreferrer', class: 'twitter-share' do %>
      <% end %>
     </div>
     <div class="footer-table">
        <%= render 'shared/footer' %>
     </div>
    </div>
     <%= javascript_pack_tag 'typings/index' %>
  </div>
