<!DOCTYPE html>
<html>
  <head>
    <title>SurfingTyping</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_pack_tag 'application' %>
    <%= javascript_pack_tag 'application' %>
  </head>

  <body>
    <header>
      <%= render 'shared/header' %>
    </header>
    <%= yield %>
    <footer>
      <%= render 'shared/footer' %>
    </footer>
  </body>
</html>
